{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Flutter: pub get & analyze (w5_flutter_app)",
			"type": "shell",
			"command": "powershell",
			"args": [
				"-NoProfile",
				"-NonInteractive",
				"-Command",
				"flutter pub get; flutter analyze"
			],
			"problemMatcher": [
				"$flutter"
			],
			"group": "build"
		}
	]
}